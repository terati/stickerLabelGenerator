{
    "manifest_version": 3,
    "name": "CRXJS React Vite Example",
    "version": "1.0.0",
    "permissions": ["scripting", "tabs", "windows"],
    "host_permissions": ["http://*/*", "https://*/*"],
    "action": { 
        "default_popup": "index.html"
    },
    "background": {
      "service_worker": "service_worker.js"
    },
    
    "web_accessible_resources": [
        {
            "resources": [ "icons/*.png"],
            "matches": []
        }
    ]
}