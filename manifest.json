{
    "manifest_version": 3,
    "name": "autorx",
    "version": "1.0.0",
    "permissions": ["scripting", "tabs", "activeTab", "windows", "storage"],
    "host_permissions": ["<all_urls>"],
    "action": { 
        "default_popup": "index.html"
    },
    "background": {
      "service_worker": "service_worker.js"
    },
    "web_accessible_resources": [
        {
            "resources": [ "icons/*.png"],
            "matches": []
        }
    ]
}

